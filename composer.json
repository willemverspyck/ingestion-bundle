{
    "name": "spyck/ingestion-bundle",
    "description": "Ingestion Bundle for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Verspyck",
            "email": "willemverspyck@outlook.com",
            "homepage": "https://verspyck.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "ingestion",
        "php"
    ],
    "homepage": "https://github.com/willemverspyck/ingestion-bundle",
    "type": "library",
    "require": {
        "php": "^8.2",
        "doctrine/collections": "^2",
        "doctrine/dbal": "^3|^4",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2|^3",
        "doctrine/persistence": "^3",
        "psr/cache": "^3",
        "symfony/cache": "^6|^7",
        "symfony/config": "^6|^7",
        "symfony/console": "^6|^7",
        "symfony/dependency-injection": "^6|^7",
        "symfony/framework-bundle": "^6|^7",
        "symfony/messenger": "^6|^7",
        "symfony/property-access": "^6|^7",
        "symfony/serializer": "^6|^7",
        "symfony/validator": "^6|^7",
        "symfony/http-client": "^6|^7",
        "symfony/http-foundation": "^6|^7",
        "symfony/http-kernel": "^6|^7",
        "symfony/validator": "^6|^7",
        "twig/twig": "^3"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Spyck\\IngestionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spyck\\IngestionBundle\\Tests\\": "tests/"
        }
    }
}
